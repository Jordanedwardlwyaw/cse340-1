<header>
    <div class="header">
      <h1>CSE Motors</h1>
      
      <!-- Provide default values if variables are undefined -->
      <% if (typeof loggedIn !== 'undefined' && loggedIn) { %>
        <a href="/account/account">Manage Account</a>
        <% if (typeof accountType !== 'undefined' && (accountType === 'Employee' || accountType === 'Admin')) { %>
          <a href="/inv">Inventory Management</a>
        <% } %>
        <a href="/account/logout">Logout</a>
      <% } else { %>
        <a href="/account/login">My Account</a>
      <% } %>
    </div>
</header>